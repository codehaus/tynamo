	<component-type type="LocaleLink" specification-path="/org/trails/link/LocaleLink.jwc"/>

	<component-type type="BlobComponent" specification-path="blob/BlobComponent.jwc"/>
	<component-type type="TrailsUpload" specification-path="blob/TrailsUpload.jwc"/>
	<component-type type="TrailsDownload" specification-path="blob/TrailsDownload.jwc"/>
