<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<div xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd" xmlns:p="tapestry:parameter">

	<t:if test="renderType.image">
		<t:if test="notNull">
			<img src="${blobLink}" alt="${fileName}"/>
			<p:else>
				<img src="${noImage}"/>
			</p:else>
		</t:if>
	</t:if>

	<t:if test="renderType.icon">
		<t:if test="notNull">
			<a href="${blobLink}">
				<img src="${noIcon}"/>
			</a>
			<p:else>
				<img src="${noIcon}"/>
			</p:else>
		</t:if>
	</t:if>

	<t:if test="notImageNotIconNotNull">
		<a href="${blobLink}">
			${fileName}
		</a>
	</t:if>

</div>
